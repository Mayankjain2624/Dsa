class Solution {
public:
    int xorAllNums(vector<int>& nums1, vector<int>& nums2) {
        long long int n1 = nums1.size();
        long long int n2 = nums2.size();
            long long int ans = 0;
        if (n1 % 2 != 0) {
            for (auto& it : nums2) {
                ans ^= it;
            }
            if(n2%2==0)
            return ans;
        }
        if(n2 %2!=0){
            for (auto& it : nums1) {
                ans ^= it;
            }
            if(n1%2==0)
            return ans;
        }
        else 
        return ans;
        return ans;
    }
};
